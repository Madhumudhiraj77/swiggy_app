{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "build"  
        }
      },
      {
        "src": "server/index.js",
        "use": "@vercel/node"  
      }
    ],
    "routes": [
      {
        "src": "/proxy",
        "dest": "/server/index.js"
      },
      {
        "src": "/(.*)",
        "dest": "/build/$1" 
      }
    ]
  }
  